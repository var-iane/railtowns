		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.11",
		"d3": "^7.9.0",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"d3-scale-chromatic": "^3.1.0",
